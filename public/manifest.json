{
  "manifest_version": 3,
  "name": "UpFetch",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "activeTab",
    "notifications",
    "alarms",
    "storage",
    "scripting"
  ],
  "host_permissions": ["*://*/*"]

}
